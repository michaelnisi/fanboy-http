{
  "name": "fanboy-http",
  "version": "4.0.1",
  "description": "search iTunes for podcast feeds",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "standard",
    "test": "tap test/*.js",
    "posttest": "rm -rf /tmp/fanboy-http-[1-9]*",
    "start": "FANBOY_LOG_LEVEL=30 node start.js | node_modules/bunyan/bin/bunyan -o short"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/michaelnisi/fanboy-http.git"
  },
  "keywords": [
    "fanboy",
    "http"
  ],
  "bugs": {
    "url": "https://github.com/michaelnisi/fanboy-http/issues"
  },
  "homepage": "https://github.com/michaelnisi/fanboy-http",
  "engines": {
    "node": ">=6"
  },
  "author": "Michael Nisi <michael.nisi@gmail.com> (http://troubled.pro)",
  "dependencies": {
    "bunyan": "^1.8.5",
    "fanboy": "7.0.1",
    "http-hash": "^2.0.0",
    "http-methods": "^1.1.0",
    "mkdirp": "^0.5.1",
    "negotiator": "^0.6.1",
    "readable-stream": "^3.4.0"
  },
  "devDependencies": {
    "binary-split": "^1.0.5",
    "nock": "^11.4.0",
    "rimraf": "^2.6.1",
    "standard": "^14.3.1",
    "tap": "^14.6.9"
  },
  "license": "MIT"
}
